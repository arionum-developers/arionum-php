{
    "name": "pxgamer/arionum-php",
    "type": "library",
    "description": "An API wrapper for the Arionum cryptocurrency node.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "stephenhill/base58": "^1.1"
    },
    "require-dev": {
        "illuminate/support": "^5.8",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\Arionum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\Arionum\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "pxgamer\\Arionum\\Laravel\\ArionumServiceProvider"
            ],
            "aliases": {
                "Arionum": "pxgamer\\Arionum\\Laravel\\ArionumFacade"
            }
        }
    }
}
