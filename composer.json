{
    "name": "owenvoke/arionum-php",
    "type": "library",
    "description": "An API wrapper for the Arionum cryptocurrency node.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "stephenhill/base58": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\Arionum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\Arionum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
